@@ -157,59 +172,57 @@ task javadocJar(type: Jar) {
 
 build.dependsOn javadocJar
 
-license {
-	header file("HEADER")
-	include '**/*.java'
-
-	// Exclude gson since it is google's code, we just modify and bundle it
-	exclude '**/lib/gson/*.java'
+shadowJar {
+	// Copied from Hotpur-Server
+	relocate 'jline', 'org.bukkit.craftbukkit.libs.jline'
+	relocate 'org.apache.commons.codec', 'org.bukkit.craftbukkit.libs.org.apache.commons.codec'
+	relocate 'org.apache.commons.io', 'org.bukkit.craftbukkit.libs.org.apache.commons.io'
+	relocate 'org.apache.commons.lang3', 'org.bukkit.craftbukkit.libs.org.apache.commons.lang3'
+	relocate 'org.objectweb.asm', 'org.bukkit.craftbukkit.libs.org.objectweb.asm'
+	relocate('org.bukkit.craftbukkit', "org.bukkit.craftbukkit.v${project.nms_revision}"){
+		exclude 'org.bukkit.craftbukkit.Main*'
+	}
+	relocate 'net.minecraft.server', "net.minecraft.server.v${project.nms_revision}"
+	transform(com.github.jengelman.gradle.plugins.shadow.transformers.Log4j2PluginsCacheFileTransformer)
 }
 
-publishing {
-	publications {
-		mavenJava(MavenPublication) {
-			// add all the jars that should be included when publishing to maven
-			artifact(file("${project.buildDir}/libs/$archivesBaseName-${version}.jar")) {
-				builtBy remapJar
-			}
-			artifact(sourcesJar) {
-				builtBy remapSourcesJar
-			}
-			artifact javadocJar
-			artifact(file('src/main/resources/fabric-installer.json')) {
-				builtBy remapJar
-			}
-			artifact(file('src/main/resources/fabric-installer.launchwrapper.json')) {
-				builtBy remapJar
-				classifier = "launchwrapper"
-			}
-		}
+assemble.dependsOn(shadowJar)
+
+task genPatches(type: MakePatchesTask) {
+	originalJar rootProject.file("./Purpur/Paper/work/Minecraft/${project.minecraft_version}/${project.minecraft_version}-m.jar")
+	targetJar project(":hotpur").buildDir.toPath().resolve("libs").resolve("hotpur-${project.minecraft_version}.jar").toFile()
+	setRelocations(
+			Arrays.asList(
+					new PatchesMetadata.Relocation('jline', 'org.bukkit.craftbukkit.libs.jline', true),
+					new PatchesMetadata.Relocation('org.apache.commons.codec', 'org.bukkit.craftbukkit.libs.org.apache.commons.codec', true),
+					new PatchesMetadata.Relocation('org.apache.commons.io', 'org.bukkit.craftbukkit.libs.org.apache.commons.io', true),
+					new PatchesMetadata.Relocation('org.apache.commons.lang3', 'org.bukkit.craftbukkit.libs.org.apache.commons.lang3', true),
+					new PatchesMetadata.Relocation('org.objectweb.asm', 'org.bukkit.craftbukkit.libs.org.objectweb.asm', true),
+					new PatchesMetadata.Relocation('', "net.minecraft.server.v${project.nms_revision}", false)
+			) as Set<PatchesMetadata.Relocation>
+	)
+
+	dependsOn project(":hotpur").getTasks().getByPath("shadowJar")
+
+	doLast {
+		final Properties prop = new Properties()
+		prop.setProperty("minecraftVersion", project.minecraft_version)
+		PropertiesUtils.saveProperties(prop, getOutputDir().toPath().getParent().resolve("hotpur-launch.properties"), "Hotpur launch values")
 	}
+}
 
-	// select the repositories you want to publish to
-	repositories {
-		if (ENV.MAVEN_URL) {
-			maven {
-				url ENV.MAVEN_URL
-				credentials {
